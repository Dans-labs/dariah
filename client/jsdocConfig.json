{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./docs/gen",
    "template": "node_modules/docdash"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "idInHeading": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  }
}
